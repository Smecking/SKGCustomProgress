<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:smecking="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.smecking.customprogress.MainActivity">

    <com.smecking.customprogress.CustomProgress
        android:id="@+id/custom_progress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        smecking:reach_color="#9b4400"
        smecking:unreach_color="#b35c44"
        smecking:text_color="#312520"
        android:max="100"
        android:progress="30"
        android:padding="5dp"
        />
</RelativeLayout>
